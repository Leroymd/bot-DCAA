{
  "name": "fractal-scalp-trading-bot",
  "version": "1.0.0",
  "description": "Торговый бот для Bitget на основе фрактальной стратегии",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-client": "cd client && npm install",
    "install-all": "npm install && npm run install-client",
    "dev-all": "concurrently \"npm run dev\" \"npm run client\""
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.2",
    "bitget-api": "^2.3.5",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/node": "^22.15.17",
    "@types/react": "^19.1.3",
    "@types/react-dom": "^19.1.3",
    "@types/react-router-dom": "^5.3.3",
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.1",
    "typescript": "^5.8.3"
  }
}
